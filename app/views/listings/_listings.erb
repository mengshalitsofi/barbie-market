<%if listings.count > 0%>
  <table border=1>
    <tr>
      <% if index %>
      <td>
        <b>
          Barbie Name
        </b>
      </td>
      <%end%>
      <td>
        <b>
          Quantity
        </b>
      </td>
      <td>
        <b>
          Sold By
        </b>
      </td>
    </tr>
    <%listings.each do |l|%>
      <% if l.product %>
        <tr>
          <% if index%>
            <td>
              <%= l.product.name%>
            </td>
          <% end %>
          <td>
            <%=l.quantity%>
          </td>
          <td>
            <%=l.user.username%>
          </td>
        </tr>
      <%end%>
    <%end%>
  </table>
<%end%>