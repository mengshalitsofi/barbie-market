<h1>Sign Up as a Barbie Seller!</h1>

<%=render(partial: "application/errors", locals: {errors: @user.errors.full_messages})%>

<%= form_for @user, url: signup_path do |f| %>
  <%=f.label "Username"%>
  <%=f.text_field :username%>
  <%=f.label "Password"%>
  <%=f.password_field :password %>
  <%=f.label "Password Confirmation"%>
  <%=f.password_field :password_confirmation %>
  <%=f.submit "Sign Up"%>

<%end%>

<%=link_to "Sign Up With Facebook", '/auth/facebook'%>